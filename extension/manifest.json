{
    "manifest_version":3,
    "name":"try",
    "icons":{
        "32":"popup.png"
    },
    "version": "1.0.0",
    "background":{
        "service_worker":"service-worker.js",
        "type":"module"
    },
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting","tabs","activeTab"
    ],
    "host_permissions":[
        "http://*//*",
        "https://*//*"
    ]
}